class Solution {
public:
    int maxFrequencyElements(vector<int>& nums) {
        map<int,int>mp;
        int mx=INT_MIN;
        for(auto x:nums)mp[x]++;
        for(auto x:mp)mx=max(mx,x.second);
        int ans=0;
        for(auto x:mp){
            if(x.second==mx)ans+=x.second;
        }
        return ans;
    }
};
