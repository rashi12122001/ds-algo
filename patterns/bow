// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
 int n;
 cin>>n;
 int star=1;
 int sp=n-2;
		for(int i = 1 ; i <=n ; i++)
		{
		    for(int k=1;k<=star;k++){
		        cout<<'*';
		    }
		    for(int space=0;space<sp;space++){
		    cout<<' ';
		}
			for(int j = 0 ; j <star ; j++)
			{   if(j!=(n-1)/2){
				cout<<'*';
			}
			}
				if(i<(n+1)/2){
				    star++;
				    sp=sp-2;
				}
				else{
				    star--;
				    sp+=2;
				}
			
			cout<<endl;
		}
}
